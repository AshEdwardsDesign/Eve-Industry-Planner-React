rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  match /JobPlanner/{documentID}{
     allow read: if request.auth.uid == documentID;
   allow write: if request.auth.uid == documentID;
   }
    match /JobPlanner/{documentID}/Jobs/{documents}{ 
   allow read: if request.auth.uid == documentID;
   allow write: if request.auth.uid == documentID;
    }
  }
}